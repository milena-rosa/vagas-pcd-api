{
  "name": "@pcdvagas/server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "test:unit": "vitest run --dir src/modules",
    "test:unit:watch": "vitest --dir src/modules",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/__test__/e2e",
    "test:e2e:watch": "vitest --dir src/__test__/e2e",
    "test:coverage": "vitest run --coverage",
    "pretest:ui": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:ui": "vitest --ui",
    "lint": "eslint --ext .ts --fix src/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "1.2.0",
    "@types/bcryptjs": "2.4.2",
    "@types/node": "18.15.10",
    "@types/supertest": "2.0.12",
    "@vitest/coverage-c8": "0.29.7",
    "@vitest/ui": "0.29.8",
    "eslint": "8.36.0",
    "npm-run-all": "4.1.5",
    "prisma": "4.12.0",
    "supertest": "6.3.3",
    "tsup": "6.7.0",
    "tsx": "3.12.6",
    "typescript": "5.0.2",
    "vite-tsconfig-paths": "4.0.7",
    "vitest": "0.29.7",
    "vitest-mock-extended": "1.1.3"
  },
  "dependencies": {
    "@fastify/cookie": "8.3.0",
    "@fastify/cors": "8.2.1",
    "@fastify/jwt": "6.7.1",
    "@fastify/swagger": "8.3.1",
    "@fastify/swagger-ui": "1.6.0",
    "@prisma/client": "4.12.0",
    "axios": "1.3.4",
    "bcryptjs": "2.4.3",
    "dotenv": "16.0.3",
    "fastify": "4.15.0",
    "fastify-plugin": "4.5.0",
